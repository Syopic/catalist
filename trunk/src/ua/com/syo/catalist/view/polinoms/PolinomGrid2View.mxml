<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml" width="100%" height="100%">
<mx:Script>
	<![CDATA[
		[Bindable]
		public var title:String;
		
	]]>
	
</mx:Script>
	
	<mx:XMLList id="firstColumn">
        <col>
            <gasName>CO</gasName>
        </col>
        <col>
            <gasName>CO2</gasName>
        </col>
        <col>
            <gasName>HCs</gasName>
        </col>
        <col>
            <gasName>NOx</gasName>
        </col>
        <col>
            <gasName>O2</gasName>
        </col>
        <col>
            <gasName>Gпал</gasName>
        </col>
        <col>
            <gasName>Gпов</gasName>
        </col>
    </mx:XMLList>
	
	<mx:Label text="{title}"  fontWeight="normal"/>
	<mx:DataGrid dataProvider="{firstColumn}" width="100%" rowHeight="20" rowCount="7" editable="true" draggableColumns="false">
		<mx:columns>
			<mx:DataGridColumn headerText="" dataField="gasName"/>
			<mx:DataGridColumn headerText="B0" dataField="col1"/>
			<mx:DataGridColumn headerText="B1" dataField="col2"/>
			<mx:DataGridColumn headerText="B11" dataField="col3"/>
			<mx:DataGridColumn headerText="B111" dataField="col4"/>
			<mx:DataGridColumn headerText="B1111" dataField="col5"/>
			<mx:DataGridColumn headerText="B11111" dataField="col6"/>
		</mx:columns>
	</mx:DataGrid>
</mx:VBox>
